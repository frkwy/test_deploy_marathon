{
  "id": "bridged-webapp-django12",
  "cpus": 0.1,
  "mem": 0.1,
  "labels": {
      "name": "test1",
      "environment": "staging"
  },
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "yusukefurukawa/test:latest",
      "network": "BRIDGE",
      "portMappings": [
        {  "protocol": "tcp", "hostPort": 0, "containerPort": 8000}
      ]
    }
  },
  "forcePullImage" : true,
  "args": ["python", "manage.py", "runserver", "0.0.0.0:8000"]
}

